{
  "name": "social-media-api",
  "version": "1.0.0",
  "description": "Social media api like twitter.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start:prod": "node build/server.js",
    "start:consumer": "node build/lib/queue/consumer.js",
    "build": "tsup src --out-dir build --format esm",
    "biome:check": "biome check --write .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "migrate:generate": "drizzle-kit generate",
    "migrate:push": "drizzle-kit push",
    "migrate:up": "drizzle-kit up",
    "migrate:check": "drizzle-kit check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.7.0",
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/helmet": "13.0.1",
    "@fastify/rate-limit": "10.2.2",
    "@fastify/swagger": "9.4.2",
    "@fastify/swagger-ui": "5.2.2",
    "@paralleldrive/cuid2": "2.2.2",
    "amqplib": "0.10.7",
    "bcryptjs": "3.0.2",
    "dotenv": "16.4.7",
    "drizzle-orm": "0.41.0",
    "fastify": "5.2.2",
    "nodemailer": "6.10.0",
    "pino": "9.6.0",
    "postgres": "3.4.5",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/amqplib": "0.10.7",
    "@types/bcryptjs": "3.0.0",
    "@types/node": "22.14.0",
    "@types/nodemailer": "6.4.17",
    "@types/pg": "8.11.11",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "3.1.1",
    "drizzle-kit": "0.30.6",
    "pino-pretty": "13.0.0",
    "supertest": "7.1.0",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  }
}
